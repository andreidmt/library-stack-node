{
	"compilerOptions": {
    "target": "es2021",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "incremental": true
	},
	"include": [
		"src"
	],
  "exclude": [
		"**/node_modules",
		"**/.*/"
	]
}
